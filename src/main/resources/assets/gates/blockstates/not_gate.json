{
  "multipart": [
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "gates:block/gate/not_gate", "y": 0
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "gates:block/gate/not_gate", "y": 90
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "gates:block/gate/not_gate", "y": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "gates:block/gate/not_gate", "y": 270
      }
    },


    {
      "when": {
        "AND": [
          {"facing": "south"},
          {"powered": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_on", "y": 0
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "west"},
          {"powered": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_on", "y": 90
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "north"},
          {"powered": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_on", "y": 180
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "east"},
          {"powered": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_on", "y": 270
      }
    },


    {
      "when": {
        "AND": [
          {"facing": "south"},
          {"powered": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_off", "y": 0
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "west"},
          {"powered": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_off", "y": 90
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "north"},
          {"powered": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_off", "y": 180
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "east"},
          {"powered": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/output_torch_off", "y": 270
      }
    },



    {
      "when": {
        "AND": [
          {"facing": "south"},
          {"input": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_on", "y": 0
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "west"},
          {"input": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_on", "y": 90
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "north"},
          {"input": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_on", "y": 180
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "east"},
          {"input": true}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_on", "y": 270
      }
    },


    {
      "when": {
        "AND": [
          {"facing": "south"},
          {"input": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_off", "y": 0
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "west"},
          {"input": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_off", "y": 90
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "north"},
          {"input": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_off", "y": 180
      }
    },
    {
      "when": {
        "AND": [
          {"facing": "east"},
          {"input": false}
        ]
      },
      "apply": {
        "model": "gates:block/torch/input_torch_off", "y": 270
      }
    }


  ]
}