{
  "multipart": [
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "gates:block/gate/nor_gate", "y": 0
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "gates:block/gate/nor_gate", "y": 90
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "gates:block/gate/nor_gate", "y": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "gates:block/gate/nor_gate", "y": 270
      }
    },
    {
      "when": {
        "powered": "true"
      },
      "apply": {
        "model": "gates:block/redstone_block/small_redstone_block_on"
      }
    },
    {
      "when": {
        "powered": "false"
      },
      "apply": {
        "model": "gates:block/redstone_block/small_redstone_block_off"
      }
    }
  ]
}